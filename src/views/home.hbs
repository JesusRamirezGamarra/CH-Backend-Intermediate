<section class="bg-white dark:bg-gray-900" >    
  <div class="py-8 lg:py-4 px-4 mx-auto max-w-md">
    {{#if user }}
    <h1 class="mb-4 text-4xl tracking-tight font-extrabold text-center text-gray-900 dark:text-white">User CODER SHOP</h1>
    <h2 class="mb-4 text-xl tracking-tight font-extrabold text-center text-gray-900 dark:text-white">Welcome, {{user.name}} </h2>
    {{ else }}    
    <h1 class="mb-4 text-4xl tracking-tight font-extrabold text-center text-gray-900 dark:text-white">CODER SHOP</h1>
    {{/if }}
    <h4 class="mb-4 text-md tracking-tight text-center text-gray-900 dark:text-white">remember that everything you collaborate will be to continue with our adoption program for a single breed pet</h4>
  </div>  
</section>
    <hr>
<div class="flex">
  <div class="w-full h-full overflow-y-auto">
    <div >
    {{!--  class="flex items-center justify-center" --}}
      <main>
        <section class="bg-white dark:bg-gray-900" >    
          <div class="px-4 mx-auto max-w-md">
            {{#if data }}
              <h4 class="mb-4 text-md tracking-tight text-center text-gray-900 dark:text-white">Found {{data.length}} products </h4>    
            {{ else }}       
              <h3 class="mb-4 text-4xl tracking-tight font-extrabold text-center text-gray-900 dark:text-white">Product Not Found</h3>
            {{/if }}
          </div>  
        </section>          
        <section class="pb-8">
          <div class="bg-white">
            <div class="mx-auto max-w-2xl py-4 px-4 sm:py-4 sm:px-6 lg:max-w-7xl lg:px-8">
              {{!-- <h2 class="sr-only">Products</h2> --}}
              <div class="grid 
              xs:grid-cols-1 gap-y-10 gap-x-2 
              sm:grid-cols-2 gap-y-10 gap-x-2 
              md:grid-cols-3 gap-y-10 gap-x-2 
              lg:grid-cols-4 gap-y-10 gap-x-2 
              xl:grid-cols-5 gap-y-10 gap-x-2 
              2xl:grid-cols-6 gap-y-10 gap-x-2
              3xl:grid-cols-7 gap-y-10 gap-x-2
              4xl:grid-cols-8 gap-y-10 gap-x-2
              5xl:grid-cols-9 gap-y-10 gap-x-2
              ">
                {{#each data}}
                <a href="#" class="group">
                  <div class="aspect-w-1 aspect-h-1 w-full overflow-hidden rounded-lg bg-gray-200 xl:aspect-w-7 xl:aspect-h-8">
                    <img src="{{thumbnail}}" alt="{{name}}" class="h-full w-full object-cover object-center group-hover:opacity-75">
                  </div>
                  <h3 class="mt-4 text-sm text-gray-700">{{name}}</h3>
                  <p class="mt-1 text-lg font-medium text-gray-900">${{price}}</p>
                </a>
                {{/each}}
                <!-- More products... -->
              </div>
            </div>
          </div>
        </section>
      </main>
    </div>
  </div>      
  <div class="flex flex-col w-64 h-screen py-8 overflow-y-auto border-r">
  {{!-- <div > --}}
    {{!-- <div class="fixed top-0 left-0 right-0">Contacts</div> --}}
    <aside>
      {{!-- <div class="relative"> --}}
        {{!-- <div class="fixed top-0 left-0 right-0">Contacts</div> --}}
        <div>
          <h2 class="text-2xl font-semibold text-center text-blue-800">Shopping Cart</h2>
        </div>
        <div class="flex flex-col justify-between mt-6">
            <ul>
              <li>
                <a class="flex items-center py-2 text-gray-700 bg-gray-100 rounded-md " href="#">
                  <svg xmlns="http://www.w3.org/2000/svg" class="w-6 h-6" fill="none" viewBox="0 0 24 24"
